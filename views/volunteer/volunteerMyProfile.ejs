    <div class="InsideAddAnimal">

        <div id="outerdiv">

            <div id="divPhoto">
                <h2 id="pic">Photo:</h2>
                <img id="photo" src="<%= data[0].img_par%>">
            </div>

            <div id="divOther">
                <div>
                    <h2 id="firstname">First Name:</h2>
                </div>

                <div>

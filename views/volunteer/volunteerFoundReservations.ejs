<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div id="datareservations">
        <% if(data.length) for(var i = 0; i < data.length; i++) {{%>
        <div id="reservationpending" onClick="is_Clicked0('<%-data[i].reservation_id%>')">
            <a><%-data_to_print[i]%></a>
            <a><%-data[i].name%></a>
            <a id="upcoming_id_icon"><i class="material-icons">chevron_right</i></a>
        </div>
        <% }} %>
    </div>

    <script>
        function is_Clicked0(id){
            url = "/volunteerReservationCard/"+id
            window.location.href = url
          }
    </script>  
</body>
</html>
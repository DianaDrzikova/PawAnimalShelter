<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

    <div id="back">
        <a id="back_link"><i class="material-icons">arrow_back</i></a>
    </div>

    <h1 id="name"><%-data[0].name%></h1>
    <hr>

    <div>
        <%- include('partials/caregiverAnimalProfile') %>
    </div>
    
        
    <script>

        var element = document.getElementById('back_link');
        //element.setAttribute('href', document.referrer);

        element.onclick = function() {
            var url = String(document.referrer)
            console.log(url)
            if(url.includes("caregiverPendingReservationProfile")){
                history.back();
                return false;
            }else{
                window.location.href = "/caregiverAnimals/?name=all&spieces=all"
            }
        }

        

        //history.pushState("<%-data%>", "Paw Animal Shelter", "<%-data[0].id%>");
        //history.replaceState("<%-data%>", "Paw Animal Shelter", "<%-data[0].name%>");

        function is_Clicked(path){
            url = path + "<%-data[0].animal_id%>"
            console.log(path)
            window.location.href = url
        }

        // Close the dropdown menu if the user clicks outside of it
        window.onclick = function(event) {
            if (!event.target.matches('.dropbtn1')) {
                var dropdowns = document.getElementsByClassName("dropdown-content1");
                var i;
                for (i = 0; i < dropdowns.length; i++) {
                    var openDropdown = dropdowns[i];
                    if (openDropdown.classList.contains('show')) {
                        openDropdown.classList.remove('show');
                    }
                }
            }

            if (!event.target.matches('.dropbtn2')) {
                var dropdowns = document.getElementsByClassName("dropdown-content2");
                var i;
                for (i = 0; i < dropdowns.length; i++) {
                    var openDropdown = dropdowns[i];
                    if (openDropdown.classList.contains('show')) {
                        openDropdown.classList.remove('show');
                    }
                }
            }
        }




    </script>
</body>
</html>